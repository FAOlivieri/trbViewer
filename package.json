{
  "name": "trbviewer",
  "displayName": "TRB Viewer",
  "publisher": "FAOlivieri",
  "description": "Unpickles and displays .trb files generated by RFDiffusion",
  "version": "0.3.0",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDidOpenTextDocument",
    "onLanguage:trb",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "trb",
        "extensions": [
          ".trb"
        ],
        "aliases": [
          "TRB"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FAOlivieri/trbViewer.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.0"
  }
}
